!!!
html
	head
		title= "Music Selector"
		script(src='/vendor/jquery-2.0.3.js')
		script(src='/vendor/jquery-ui.js')
		script(src='/socket.io/socket.io.js')
		script(src='http://connect.soundcloud.com/sdk.js')
		script(src='utils.js')
		script(src='/selector.js')
		link(rel='stylesheet', href='/stylesheets/jquery-ui.css')
		link(rel='stylesheet', href='/stylesheets/style.css')

	body
		.input
			.soundcloud
				input#soundcloudLinkField(style='width:350px;')
				input#soundcloudPlayButton(type='button', value='Play Soundcloud Link')
				input#soundcloudResumeButton(type='button', value='Resume Soundcloud')
				#soundcloudVolumeSlider.volumeSlider.soundcloud
				#soundcloudPlaybackSlider.playbackSlider.soundcloud
				p#soundcloudTitle.songTitle
				p#soundcloudProgressText.playerProgressText
				input#soundcloudSearchQuery
				input#soundcloudSearchButton(type='button', value='Search Soundcloud')
				ul#soundcloudSearchResult.searchResults

			.youtube
				input#youtubeVideoIdField(style='width:350px;')
				input#youtubePlayButton(type='button', value='Play Youtube Video')
				input#youtubeResumeButton(type='button', value='Resume Youtube')
				#youtubeVolumeSlider.volumeSlider.youtube
				#youtubePlaybackSlider.playbackSlider.youtube
				p#youtubeTitle.songTitle
				p#youtubeProgressText.playerProgressText
				input#youtubeSearchQuery
				input#youtubeSearchButton(type='button', value='Search Youtube')
				ul#youtubeSearchResult.searchResults
		br
		input#pauseAll(type='button', value='Pause')
