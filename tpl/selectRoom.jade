!!!
html
	head
		title="Create / Select Room"
		script(src='/vendor/jquery-2.0.3.js')
		script(src='/vendor/jquery-ui.js')
		script(src='/utils.js')
		script(src='/selectRoom.js')
		link(rel='stylesheet', href='/stylesheets/jquery-ui.css')
		link(rel='stylesheet', href='/stylesheets/style.css')

	body
		.content
			select#roomSelector
				for item in rooms
					option #{item}
			br
			br
			input#goToSelector(type='button', value='Go to selector')
			br
			br
			input#goToPlayer(type='button', value='Go to player')